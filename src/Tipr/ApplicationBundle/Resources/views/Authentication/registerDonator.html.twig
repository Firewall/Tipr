{#post documentNumber and birthDate to tipr_application_loginProcess#}

<form class="form-horizontal" role="form" action="{{ path('tipr_application_registerDonatorProcess') }}" {{ form_enctype(form) }} method="post"
      novalidate="novalidate">

    <div class="form-group">
        {{ form_label(form.birthDate,null,{'label_attr': {'class': 'col-sm-2' }}) }}
        <div class="col-sm-9">
            {{ form_widget(form.birthDate, {'attr': {'class': 'form-control' }}) }}
            {{ form_errors(form.birthDate) }}
        </div>
    </div>

    <div class="form-group">
        {{ form_label(form.documentNumber,null,{'label_attr': {'class': 'col-sm-2' }}) }}
        <div class="col-sm-10">
            {{ form_widget(form.documentNumber, {'attr': {'class': 'form-control' }}) }}
            {{ form_errors(form.documentNumber) }}
        </div>
    </div>

    <div class="form-group">
        {{ form_label(form.code,null,{'label_attr': {'class': 'col-sm-2' }}) }}
        <div class="col-sm-9">
            {{ form_widget(form.code, {'attr': {'class': 'form-control' }}) }}
            {{ form_errors(form.code) }}
        </div>
    </div>

    {{ form_widget(form.send,{'attr' : {'class' : 'btn btn-primary' }}) }}

    {{ form_widget(form._token) }}
</form>